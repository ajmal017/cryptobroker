#!/usr/bin/env ruby

require 'cryptobroker'
require 'cryptobroker/indicator/dema'

app = Cryptobroker.new('../config.yml')
trades = app.trades
tf = 15 * 60
dema = Cryptobroker::Indicator::DEMA.new []
trades.each do |market,trade|
  chart = Cryptobroker::OHLCV.create trade, tf, nil, nil, false
  puts market
  dema.plot chart
end