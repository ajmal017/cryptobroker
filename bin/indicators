#!/usr/bin/env ruby

require_relative '../lib/cryptobroker/evaluator/config'

config = Cryptobroker::Evaluator::Config.new('../tmp/indicator_test_case.yml')
puts '--- indicators ---'
puts config.indicators.map(&:to_s)
(Cryptobroker::Evaluator::Config::ENTRIES - [:indicators]).each do |entry|
  puts "--- #{entry} ---"
  puts config.public_send(entry).to_s
end

