#!/usr/bin/env ruby

require_relative '../lib/cryptobroker/evaluator'

evaluator = Cryptobroker::Evaluator.new('../tmp/indicator_test_case.yml')
puts '--- indicators ---'
puts evaluator.indicators.map(&:to_s)
(Cryptobroker::Evaluator::Config::ENTRIES - [:indicators]).each do |entry|
  puts "--- #{entry} ---"
  puts evaluator.public_send(entry).to_s
end

